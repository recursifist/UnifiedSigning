{
  "$schema": "./schema.json",
  "webpages": [
    {
      "title": "Existential Safety Pledge",
      "url": "https://existentialsafetypledge.org/",
      "auto": "submit",
      "actions": [
        {
          "#sign": "scrollTo"
        }
      ],
      "submit": "form button[type='submit']",
      "fields": [
        {
          "id": "full-name",
          "label": "Full Name",
          "required": true,
          "querySelector": "form input[placeholder='Full Name / Organization Name*']",
          "inputType": "text"
        },
        {
          "id": "email",
          "label": "Email",
          "required": true,
          "querySelector": "form input[placeholder='Email Address*']",
          "inputType": "text"
        },
        {
          "id": "existential-safety-pledge-consent",
          "label": "[Existential Safety Pledge]\nI/we pledge that I/we will not intentionally contribute to any effort that I/we believe will result in a net risk of 0.1% or higher of causing humanity's extinction.\nI/we understand that the information submitted above will be made public while my/our email address will remain confidential and used solely for communications related to this pledge. I/we are participating voluntarily and may withdraw my/our consent at any time by contacting the pledge organizers.",
          "required": true,
          "querySelector": "form input[name='Consent']",
          "inputType": "checkbox"
        }
      ]
    },
    {
      "title": "international AI Governance Petition",
      "url": "https://aigovernancepetition.org/",
      "auto": "submit",
      "actions": [
        {
          "#sign-the-petition": "scrollTo"
        }
      ],
      "submit": "form button[type='submit']",
      "fields": [
        {
          "id": "full-name",
          "label": "Full Name",
          "required": true,
          "querySelector": "form input[placeholder='Full Name*']",
          "inputType": "text"
        },
        {
          "id": "email",
          "label": "Email",
          "required": true,
          "querySelector": "form input[placeholder='Email Address*']",
          "inputType": "text"
        },
        {
          "id": "profession",
          "label": "Profession",
          "required": true,
          "querySelector": "form input[placeholder='Profession*']",
          "inputType": "text"
        },
        {
          "id": "affiliation",
          "label": "Affiliation",
          "required": true,
          "querySelector": "form input[placeholder='Affiliation*']",
          "inputType": "text"
        },
        {
          "id": "International-ai-governance-petition-consent",
          "label": "[International AI Governance Petition]\nBy signing, I agree to join a global community of people advocating for responsible AI governance. \nI understand that the information submitted above will be made public, except for my email which will remain confidential and used solely for Center for Existential Safety communications.",
          "required": true,
          "querySelector": "form input[name='Consent']",
          "inputType": "checkbox"
        }
      ]
    },{
