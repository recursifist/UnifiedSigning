      "title": "Existential Safety Pledge",
      "url": "https://existentialsafetypledge.org/",
      "auto": "submit",
      "actions": [
        {
          "#sign": "scrollTo"
        }
      ],
      "submit": "form button[type='submit']",
      "fields": [
        {
          "id": "full-name",
          "label": "Full Name",
          "required": true,
          "querySelector": "form input[placeholder='Full Name / Organization Name*']",
          "inputType": "text"
        },
        {
          "id": "email",
          "label": "Email",
          "required": true,
          "querySelector": "form input[placeholder='Email Address*']",
          "inputType": "text"
        },
        {
          "id": "-consent-existential-safety-pledge",
          "label": "I/we pledge that I/we will not intentionally contribute to any effort that I/we believe will result in a net risk of 0.1% or higher of causing humanity's extinction. I/we understand that the information submitted above will be made public while my/our email address will remain confidential and used solely for communications related to this pledge. I/we are participating voluntarily and may withdraw my/our consent at any time by contacting the pledge organizers.",
          "required": true,
          "querySelector": "form input[name='Consent']",
          "inputType": "checkbox"
        }
      ]
    },
